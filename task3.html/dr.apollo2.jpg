    <script>
      function getOS() {
          const userAgent = window.navigator.userAgent.toLowerCase();
          const macosPlatforms = /(macintosh|macintel|macppc|mac68k|macos)/i;
          const windowsPlatforms = /(win32|win64|windows|wince)/i;
          const iosPlatforms = /(iphone|ipad|ipod)/i;
          let os = null;

          if (macosPlatforms.test(userAgent)) {
            os = 'macos';
          } else if (iosPlatforms.test(userAgent)) {
            os = 'ios';
          } else if (windowsPlatforms.test(userAgent)) {
            os = 'windows';
          } else if (/android/.test(userAgent)) {
            os = 'android';
          } else if (!os && /linux/.test(userAgent)) {
            os = 'linux';
          }

          return os;
        }

        document.addEventListener('DOMContentLoaded', function () {
          const os = getOS();
          if (os === 'windows') {
            const browserZoomLevel = Math.round(window.devicePixelRatio * 100);
            
            // if (browserZoomLevel > 100 && window.location.href.indexOf('apollo-prohealth') !== -1 &&  window.location.href.indexOf('apollo-prohealth/payment') ==-1 ) {
            //   document.body.style.zoom = '65%';
            // } else {
            //   document.body.style.zoom = '100%';
            // }
             if (browserZoomLevel > 100 && window.location.pathname.toLowerCase() =='/apollo-prohealth' ||
                window.location.pathname.toLowerCase().indexOf('/apollo-prohealth?') !== -1  || 
                window.location.pathname.toLowerCase().indexOf('/apollo-prohealth/') !== -1  ) 
             {
             document.body.style.zoom = '60%';
